<h1> 已收藏工作 </h1>

<table class="table table-boldered">
  <thead>
        <tr>
            <td>
                职缺</td>
            <td>工作地点</td>
            <td>薪资下限</td>
            <td>薪资上限</td>
            <td>刊登时间</td>
        </tr>

    </thead>
     <tbody>
  <% @jobs.each do |job| %>
  <tr>

    <td>
      <%= link_to(job.title, job_path(job)) %>
    </td>
    <td>
      <%= job.workplace %>
    </td>
    <td>
        <%= job.wage_lower_bound %>
    </td>
    <td>
        <%= job.wage_upper_bound %>
    </td>
    <td>
      <%= job.created_at %>
    </td>
  </tr>
  <% end %>
 </tbody>
</table>



<div class="text-center">
  <%= will_paginate @jobs %>
</div>
